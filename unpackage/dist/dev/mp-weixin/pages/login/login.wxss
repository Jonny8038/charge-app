.dlitem {
	display: flex;
	align-items: center;
	margin: 20rpx auto;
	width: 626rpx;
	height: 90rpx;
	line-height: 90rpx;
	border-bottom: 1rpx solid #DEE0EA;
}
.dlitem1 .ico {
	width: 30rpx;
	height: 40rpx;
}
.dlitem2 .ico {
	width: 30rpx;
	height: 36rpx;
}
.dlitem2 .eyeico {
	width: 34rpx;
	height: 14rpx;
}
.dlitem2 .eyekico {
	width: 34rpx;
	height: 24rpx;
}
.dlitem input {
	margin-left: 10rpx;
	flex: 1;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 30rpx;
	color: #212D43;
}
.phcolor {
	font-size: 30rpx;
	color: #7B7E8C;
}
.dltip {
	display: flex;
	justify-content: flex-end;
	width: 622rpx;
	margin: 0 auto;
	height: 60rpx;
	line-height: 60rpx;
}
.dltip text {
	font-size: 26rpx;
	color: #7B7E8C;
}
.dbtn {
	margin: 0 auto;
	margin-top: 66rpx;
	margin-bottom: 10rpx;
	width: 700rpx;
	height: 90rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #fff;
	font-weight: bold;
	text-align: center;
	background: #212D43;
	border-radius: 40rpx;
}
.dsf {
	margin-top: 30rpx;
}
.dsftit {
	font-size: 30rpx;
	color: #A6A6A6;
	text-align: center;
}
